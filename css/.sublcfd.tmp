        position: absolute;
        right: 10px;
        top: 1px;
        color: #fff;
    }
    .navigation ul ul ul{
        left: 0px;
    }
    .navigation ul > li > a:before,
    .navigation ul ul:before{
        display: none;
    }
    .navigation > ul > li:hover > a, nav.navigation > ul > li.current-menu-item > a, nav.navigation > ul > li.current-menu-ancestor > a,
    .navigation ul ul li:hover > a{
