    .navigation ul ul{
        position: inherit;
        left: 0px;
        top: auto;
        margin: 0px;
        visibility: visible;
        opacity: 1;
        float: left;
        width: 100%;
        padding: 0px 15px 0px 15px;
        background-color: transparent;
        box-shadow: none;
        -webkit-transform: translateY(0);
           -moz-transform: translateY(0);
